{
  "name": "zarf.dev",
  "version": "1.0.0",
  "private": true,
  "description": "zarf.dev",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "precommit": "husky",
    "build": "gatsby build",
    "build:staging": "npm run build && echo 'User-agent: *\nDisallow: /' > public/robots.txt",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "start": "npm run build && npm run serve",
    "develop": "npm run clean && gatsby develop",
    "typecheck": "tsc --noEmit",
    "preinstall": "npx npm-force-resolutions",
    "lint:fix": "npm run lint --fix",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx,.jsx .",
    "prettier:check": "prettier --ignore-path .gitignore --check \"./src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "format": "prettier --ignore-path .gitignore --write \"./src/**/*.{js,jsx,ts,tsx,json,md}\" .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.5.1",
    "@mui/material": "^5.5.0",
    "gatsby": "4.12.1",
    "gatsby-plugin-advanced-sitemap": "^2.1.0",
    "gatsby-plugin-image": "^2.9.1",
    "gatsby-plugin-manifest": "^4.9.1",
    "gatsby-plugin-react-helmet": "^5.9.0",
    "gatsby-plugin-react-svg": "^3.1.0",
    "gatsby-plugin-sharp": "^4.9.1",
    "gatsby-plugin-sitemap": "^5.9.0",
    "gatsby-source-filesystem": "^4.9.1",
    "gatsby-transformer-sharp": "^4.9.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "gatsby-plugin-eslint": "^4.0.2",
    "husky": "7.0.1",
    "lint-staged": "^12.3.5",
    "prettier": "^2.5.1",
    "typescript": "^4.6.2"
  },
  "resolutions": {
    "ws": "7.4.6",
    "immer": "9.0.6",
    "ansi-html": "0.0.8",
    "nth-check": "2.0.1",
    "node-fetch": "2.6.7",
    "ansi-regex": "5.0.1",
    "browserslist": "4.16.5",
    "async": "3.2.3",
    "moment": "2.29.3"
  }
}
